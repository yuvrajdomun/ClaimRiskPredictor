{
  "version": 2,
  "builds": [
    {
      "src": "vercel_minimal.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "50mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "vercel_minimal.py"
    }
  ],
  "env": {
    "PYTHON_VERSION": "3.9"
  },
  "functions": {
    "vercel_minimal.py": {
      "maxDuration": 30
    }
  }
}